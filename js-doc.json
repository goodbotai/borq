{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./lib", "./config"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "readme": "README.md",
    "tutorials": "./docs",
    "template": "./node_modules/minami",
    "encoding": "utf8",
    "destination": "./out",
    "recurse": true
  }
}
